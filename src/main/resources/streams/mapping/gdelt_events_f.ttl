  rr:predicateObjectMap [
    rr:predicate gdelt:actorType;
    rr:objectMap [ rml:reference "Actor1Type1Code" ]
  ];

  rr:predicateObjectMap [
    rr:predicate gdelt:actorType;
    rr:objectMap [ rml:reference "Actor1Type2Code" ]
  ];

  rr:predicateObjectMap [
    rr:predicate gdelt:actorType;
    rr:objectMap [ rml:reference "Actor1Type3Code" ]
  ];
.

